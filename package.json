{
  "name": "boilerplate-vite",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Ã‰verton Toffanetto",
  "description": "Project frontend with vite",
  "repository": "https://github.com/everton-dgn/boilerplate_vite",
  "main": "./src/index.js",
  "scripts": {
    "dev": "vite --mode dev",
    "build:dev": "vite build --mode dev",
    "build:prod": "vite build --mode prod",
    "start": "vite preview --mode dev --port 3001",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint --cache src --ext ts,tsx,json --max-warnings=0",
    "lint:fix": "eslint --cache --fix src --ext ts,tsx,json",
    "format": "prettier --cache --write .",
    "check:format": "prettier --cache --check .",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "vitest run",
    "test:c": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:w": "vitest",
    "test:ci": "vitest run",
    "generate": "yarn plop --plopfile componentGenerator/plopfile.js",
    "check:update": "yarn upgrade-interactive --latest",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.11.2",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@storybook/addon-a11y": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/dom": "9.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "@vitest/coverage-c8": "^0.31.4",
    "@vitest/ui": "^0.31.4",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "16.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "happy-dom": "^9.20.3",
    "husky": "8.0.3",
    "jest-styled-components": "^7.1.1",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "13.2.2",
    "plop": "^3.1.2",
    "prettier": "2.8.8",
    "storybook": "^7.0.18",
    "storybook-dark-mode": "^3.0.0",
    "storybook-mobile": "^1.0.1",
    "typescript": "5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.4"
  }
}
