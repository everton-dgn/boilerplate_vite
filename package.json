{
  "name": "boilerplate-vite",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Ã‰verton Toffanetto",
  "description": "Project frontend with vite",
  "repository": "https://github.com/everton-dgn/boilerplate_vite",
  "scripts": {
    "dev": "vite --mode dev",
    "build:dev": "vite build --mode dev",
    "build:prod": "vite build --mode prod",
    "start": "vite preview --mode dev --port 3001",
    "lint": "eslint --cache src --ext ts,tsx,json --max-warnings=0",
    "lint:fix": "eslint --cache --fix src --ext ts,tsx,json",
    "format": "prettier --cache --write src",
    "check:format": "prettier --cache --check src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:c": "vitest run --coverage",
    "test:w": "vitest",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:ci": "vitest run",
    "check:update": "yarn upgrade-interactive --latest",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "generate": "yarn plop --plopfile generator/plopFile.mjs",
    "prepare": "node scripts/create-husky-files.mjs && husky install"
  },
  "type": "module",
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fontsource/poppins": "5.0.14",
    "clsx": "2.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.23.0"
  },
  "devDependencies": {
    "eleganceui-tailwind-utilities": "5.0.0",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@storybook/addon-a11y": "8.0.10",
    "@storybook/addon-essentials": "8.0.10",
    "@storybook/addon-links": "8.0.10",
    "@storybook/react": "8.0.10",
    "@storybook/react-vite": "8.0.10",
    "@testing-library/dom": "10.1.0",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/jsdom-global": "3.0.7",
    "@types/node": "20.12.11",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "autoprefixer": "10.4.19",
    "cssnano": "7.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-love": "47.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "17.5.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sonarjs": "1.0.3",
    "eslint-plugin-tailwindcss": "3.15.1",
    "eslint-plugin-testing-library": "6.2.2",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "15.2.2",
    "plop": "4.0.1",
    "postcss": "8.4.38",
    "postcss-import": "16.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "storybook": "8.0.10",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}
